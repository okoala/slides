{
  "name": "typescript",
  "version": "1.0.0",
  "private": true,
  "author": "Koala Huang <dapixp@gmail.com>",
  "main": "dist/server.js",
  "scripts": {
    "clean": "gulp clean",
    "compile": "gulp compile",
    "build": "gulp build",
    "dev": "gulp dev",
    "start": "concurrent --kill-others \"gulp watch\" \"gulp start\""
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "express-static": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "concurrently": "^2.1.0",
    "cssnano": "^3.7.3",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-nodemon": "^2.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-open": "^2.0.0",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sequence": "^0.4.5",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^4.3.3",
    "gulp-typescript": "^2.13.6",
    "ncp": "^2.0.0",
    "tslint": "^3.5.0",
    "tslint-config-standard": "^1.3.0"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    },
    "globals": {
      "__DEVELOPMENT__": true,
      "__CLIENT__": true,
      "__SERVER__": true,
      "__DISABLE_SSR__": true,
      "__DEVTOOLS__": true,
      "socket": true,
      "webpackIsomorphicTools": true
    }
  }
}
